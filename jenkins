pipeline{
  agent any
  tools{
    maven "MAVEN_HOME"
    jdk "Java"
   }
   
   stages{
    stage('Building....'){
      steps{
        bat 'mvn clean install'
       }
      }
     stage('Testing....'){
      steps{
        bat 'mvn test'
        }
      }
     stage('Deploying.....'){
      steps{
        bat 'mvn package deploy -DmuleDeploy'
        }
       }
      }
     }
